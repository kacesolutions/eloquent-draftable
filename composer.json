{
    "name": "kace/eloquent-draftable",
    "description": "Add draftable functionality to your eloquent models.",
    "license": "MIT",
    "authors": [
        {
            "name": "Amir Marmul",
            "email": "amirmarmul@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/database": "^7.*|^8.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0",
        "orchestra/testbench": "^5.*|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Kace\\Draftable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kace\\Draftable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
